{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/user(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/chat(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/message(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  
  
