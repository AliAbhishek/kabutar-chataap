{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/user/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/chat/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/message/$1"
      }
    ]
  }
  
