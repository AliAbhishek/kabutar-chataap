{
  "version": 2,
  
  "builds": [
      { "src": "index.js", "use": "@vercel/node" }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  },
  "routes": [
      {
          "src": "/user(.*)",
          "dest": "/index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      },
      {
          "src": "/chat(.*)",
          "dest": "/index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      },
      {
          "src": "/message(.*)",
          "dest": "/index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      },
      {
          "src": "/(.*)",
          "dest": "/index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
  ]
}

